	loop 15 times:
		set {faqs.count} to {faqs.count}+1
		#if {faqs.faq%{_count}%item} is not set:
		set {faqs.%{faqs.count}%item} to skutil yaml value "FAQ%{faqs.count}%.item" from file "plugins/FAQs/faqs.yml"
		send "{faqs.%{faqs.count}%item}" to the console
		#if {faqs.faq%{_count}%name} is not set:
		set {faqs.%{faqs.count}%name} to skutil yaml value "FAQ%{faqs.count}%.name" from file "plugins/FAQs/faqs.yml"
		send "{faqs.%{faqs.count}%item}" to the console
		#if {faqs.faq%{_count}%lore} is not set:
		set {faqs.%{faqs.count}%lore} to skutil yaml value "FAQ%{faqs.count}%.lore" from file "plugins/FAQs/faqs.yml"
		send "{faqs.%{faqs.count}%item}" to the console
		#if {faqs.faq%{_count}%url} is not set:
		set {faqs.%{faqs.count}%url} to skutil yaml value "FAQ%{faqs.count}%.url" from file "plugins/FAQs/faqs.yml"
		send "{faqs.%{faqs.count}%item}" to the console
		send "FAQ%{faq.count}%" to the console
	send "&2[&aFAQs&2] &2DONE!!" to the console
	delete {faqs.count}
		






		
#Command
command /faqs:
	trigger:
		wait 1 tick
		open chest with 3 rows named "Frequently Asked Questions" to the player
		format slot 0 of player with {faqs.faq1item} named "&b%{faqs.faq1name}%" with lore "&7%{faqs.faq1lore}%" to close then run [send "&b%{faqs.faq1url}%" to the player]
		format slot 2 of player with {faqs.faq2item} named "&b%{faqs.faq2name}%" with lore "&7%{faqs.faq2lore}%" to close then run [send "&b%{faqs.faq2url}%" to the player]
