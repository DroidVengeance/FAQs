command faqsdelete:
	executable by: console
	trigger:
		loop 15 times:
			set {_count} to {_count}+1
			delete {faqs.faq%{_count}%item}
			delete {faqs.faq%{_count}%name}
			delete {faqs.faq%{_count}%lore}
			delete {faqs.faq%{_count}%url}
			send "FAQ%{_count}% Deleted!" to the console
		
			